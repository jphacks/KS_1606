doctype html

html
  head
    meta content=("text/html; charset=UTF-8") http-equiv="Content-Type" /
    meta content="IE=edge" http-equiv="X-UA-Compatible" /
    title = full_title(yield(:title))
    /! Tell the browser to be responsive to screen width
    meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport" /
    = stylesheet_link_tag "https://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css"
    = stylesheet_link_tag "http://code.ionicframework.com/ionicons/2.0.0/css/ionicons.min.css"
    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true
    = javascript_include_tag 'application', 'data-turbolinks-track' => true
    = csrf_meta_tags
  body.skin-blue.sidebar-mini data-pinterest-extension-installed="cr2.0.4" 
    .wrapper
      /! Main Header
      header.main-header
        /! Logo
        = link_to root_path ,class: "logo" do
          /! mini logo for sidebar mini 50x50 pixels
          span.logo-mini
            strong Skippo
        /! Header Navbar
        nav.navbar.navbar-static-top role="navigation" 
          /! Sidebar toggle button
          a.sidebar-toggle data-toggle="offcanvas" href="#" role="button" 
            span.sr-only Toggle navigation
          /! Navbar Right Menu
          .navbar-custom-menu
            ul.nav.navbar-nav
              /! Messages: style can be found in dropdown.less
              li.dropdown.messages-menu
                /! Menu toggle button
                a.dropdown-toggle data-toggle="dropdown" href="#" 
                  i.fa.fa-envelope-o
                  span.label.label-success 4
                ul.dropdown-menu
                  li.header You have 4 messages
                  li
                    /! inner menu: contains the messages
                    ul.menu
                      li
                        /! start message
                        a href="#" 
                        .pull-left
                          /! User Image
                          = image_tag asset_path('mac-logo.jpg') ,class: "img-circle" , size: "120x120"
                        /! Message title and timestamp
                        h4
                          | Support Team
                          small
                            i.fa.fa-clock-o
                            | 5 mins
                        /! The message
                        p Why not buy a new awesome theme?
                      /! end message
                    /! /.menu
                  li.footer
                    a href="#"  See All Messages
              /! /.messages-menu
              /! Notifications Menu
              li.dropdown.notifications-menu
                /! Menu toggle button
                a.dropdown-toggle data-toggle="dropdown" href="#" 
                  i.fa.fa-bell-o
                  span.label.label-warning 10
                ul.dropdown-menu
                  li.header You have 10 notifications
                  li
                    /! Inner Menu: contains the notifications
                    ul.menu
                      li
                        /! start notification
                        a href="#" 
                          i.fa.fa-users.text-aqua
                          | 5 new members joined today
                      /! end notification
                  li.footer
                    a href="#"  View all
              /! Tasks Menu
              li.dropdown.tasks-menu
                /! Menu Toggle Button
                a.dropdown-toggle data-toggle="dropdown" href="#" 
                  i.fa.fa-flag-o
                  span.label.label-danger 9
                ul.dropdown-menu
                  li.header You have 9 tasks
                  li
                    /! Inner menu: contains the tasks
                    ul.menu
                      li
                        /! Task item
                        a href="#" 
                          /! Task title and progress text
                        h3
                          | Design some buttons
                          small.pull-right 20%
                        /! The progress bar
                        .progress.xs
                          /! Change the css width attribute to simulate progress
                          .progress-bar.progress-bar-aqua aria-valuemax="100" aria-valuemin="0" aria-valuenow="20" role="progressbar" style=("width: 20%") 
                            span.sr-only 20% Complete
                      /! end task item
                  li.footer
                    a href="#"  View all tasks
              /! User Account Menu
              li.dropdown.user.user-menu
                /! Menu Toggle Button
                a.dropdown-toggle data-toggle="dropdown" href="#" 
                  /! The user image in the navbar
                  = image_tag asset_path('mac-logo.jpg') ,class: "user-image"
                  /! hidden-xs hides the username on small devices so only the image appears.
                  span.hidden-xs マクドナルド 新宿店
                ul.dropdown-menu
                  /! The user image in the menu
                  li.user-header
                    = image_tag asset_path('mac-logo.jpg') ,class: "img-circle"
                    p
                      | マクドナルド - 新宿店
                      small Member since Nov. 2012
                  /! Menu Body
                  li.user-body
                    .row
                      .col-xs-4.text-center
                        a href="#"  Followers
                      .col-xs-4.text-center
                        a href="#"  Sales
                      .col-xs-4.text-center
                        a href="#"  Friends
                    /! /.row
                  /! Menu Footer
                  li.user-footer
                    .pull-left
                      a.btn.btn-default.btn-flat href="#"  Profile
                    .pull-right
                      a.btn.btn-default.btn-flat href="#"  Sign out
              /! Control Sidebar Toggle Button
              li
                a data-toggle="control-sidebar" href="#" 
                  i.fa.fa-gears
      /! Left side column. contains the logo and sidebar
      aside.main-sidebar
        /! sidebar: style can be found in sidebar.less
        section.sidebar
          /! Sidebar user panel (optional)
          .user-panel
            .pull-left.image
              = image_tag asset_path('mac-logo.jpg') ,class: "img-circle"
            .pull-left.info
              p マクドナルド 新宿店
              /! Status
              a href="#" 
                i.fa.fa-circle.text-success
                | Online
          /! search form (Optional)
          form.sidebar-form action="#" method="get" 
            .input-group
              input.form-control name="q" placeholder="Search..." type="text" /
              span.input-group-btn
                button#search-btn.btn.btn-flat name="search" type="submit" 
                  i.fa.fa-search
          /! /.search form
          /! Sidebar Menu
          ul.sidebar-menu
            li.header MAIN NAVIGATION
            li.active.treeview
              = link_to '/' do
                i.fa.fa-dashboard
                span ダッシュボード
                span.pull-right-container
                  small.label.pull-right.bg-green new
            li.treeview
              = link_to information_path do
                i.fa.fa-files-o
                span 店舗情報
            li
              = link_to menu_path do
                i.fa.fa-th
                span メニュー
            li.treeview
              = link_to menu_path do
                i.fa.fa-pie-chart
                span マニュアル
                span.pull-right-container
                  i.fa.fa-angle-left.pull-right
              ul.treeview-menu
                li
                  = link_to admin_manual_dashboard_path do
                    i.fa.fa-circle-o
                    | ダッシュボード
                li
                  = link_to admin_manual_information_path do
                    i.fa.fa-circle-o
                    | 店舗情報
                li
                  = link_to admin_manual_menu_path do
                    i.fa.fa-circle-o
                    | メニュー
            li.header LABELS
            li
              a href="#" 
                i.fa.fa-circle-o.text-red
                span Important
            li
              a href="#" 
                i.fa.fa-circle-o.text-yellow
                span Warning
            li
              a href="#" 
                i.fa.fa-circle-o.text-aqua
                span Information
          /! /.sidebar-menu
        /! /.sidebar
      /! Content Wrapper. Contains page content
      .content-wrapper style=("min-height: 535px;") 
        / div style=("padding: 20px 30px; background: rgb(243, 156, 18); z-index: 999999; font-size: 16px; font-weight: 600;")
        /   a.pull-right data-placement="left" data-toggle="tooltip" href="#" style=("color: rgb(255, 255, 255); font-size: 20px;") title=("Never show me this again!")  ×
        /   a href="https://themequarry.com/" style=("color: rgba(255, 255, 255, 0.901961); display: inline-block; margin-right: 10px; text-decoration: none;") Ready to sell your theme? Submit your theme to our new marketplace now and let over 200k visitors see it!
        /   a.btn.btn-default.btn-sm href="https://themequarry.com/" style=("margin-top: -5px; border: 0px; box-shadow: none; color: rgb(243, 156, 18); font-weight: 600; background: rgb(255, 255, 255);")
        /! Content Header (Page header)
        = yield
        /! /.content
      /! /.content-wrapper
      /! Main Footer
      footer.main-footer
        /! To the right
        .pull-right.hidden-xs
          | Anything you want
        /! Default to the left
        strong
          | Copyright © 2016 .
          a href="#" Skippo 
          | All rights reserved.
      / /! Control Sidebar
      / aside.control-sidebar.control-sidebar-dark
      /   /! Create the tabs
      /   ul.nav.nav-tabs.nav-justified.control-sidebar-tabs
      /     li.active
      /       a data-toggle="tab" href="#control-sidebar-home-tab" 
      /         i.fa.fa-home
      /     li
      /       a data-toggle="tab" href="#control-sidebar-settings-tab" 
      /         i.fa.fa-gears
    /     /! Tab panes
    /     .tab-content
    /       /! Home tab content
    /       #control-sidebar-home-tab.tab-pane.active
    /         h3.control-sidebar-heading Recent Activity
    /         ul.control-sidebar-menu
    /           li
    /             a href="javascript::;" 
    /               i.menu-icon.fa.fa-birthday-cake.bg-red
    /             .menu-info
    /               h4.control-sidebar-subheading Langdon's Birthday
    /               p Will be 23 on April 24th
    /         /! /.control-sidebar-menu
    /         h3.control-sidebar-heading Tasks Progress
    /         ul.control-sidebar-menu
    /           li
    /             a href="javascript::;" 
    /             h4.control-sidebar-subheading
    /               | Custom Template Design
    /               span.pull-right-container
    /                 span.label.label-danger.pull-right 70%
    /             .progress.progress-xxs
    /               .progress-bar.progress-bar-danger style=("width: 70%") 
    /         /! /.control-sidebar-menu
    /       /! /.tab-pane
    /       /! Stats tab content
    /       #control-sidebar-stats-tab.tab-pane Stats Tab Content
    /       /! /.tab-pane
    /       /! Settings tab content
    /       #control-sidebar-settings-tab.tab-pane
    /         form method="post" 
    /           h3.control-sidebar-heading General Settings
    /           .form-group
    /             label.control-sidebar-subheading
    /               | Report panel usage
    /               input.pull-right checked="" type="checkbox" /
    /             p
    /               | Some information about this general settings option
    /           /! /.form-group
    /       /! /.tab-pane
    /   /! /.control-sidebar
    / 
    /     | Add the sidebar's background. This div must be placed
    /     | immediately after the control sidebar
    /   .control-sidebar-bg style=("position: fixed; height: auto;") 
